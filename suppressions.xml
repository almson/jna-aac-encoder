<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress files="(FdkAACLib)\.java" checks="MethodName"/>
    <suppress files="structure[/\\].+\.java"
              checks="MemberName|VisibilityModifier|JavadocVariable|JavadocMethod|MagicNumber"/>
    <suppress files="([/\\].+Test|MediaInfoSupport|TestSupport).java"
              checks="JavadocVariable|JavadocMethod|JavadocPackage|MagicNumber"/>
    <suppress files="[/\\].+Benchmark.java"
              checks="JavadocVariable|JavadocMethod|JavadocPackage|MagicNumber|MethodName|FinalClass|DesignForExtension"/>
</suppressions>
